<div class="col-sm-8">  

  <h1>New Event</h1>

  <% if @photo.errors.any? %>
    <div class="alert alert-danger">
      <ul>
      <% @photo.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <%= render 'form', photo: @photo %>

  <%= link_to 'Back', photos_path %>

</div>  