<%= form_for(photo) do |f| %>

  <div class="row">
    <div class="col-md-8">
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title, autofocus: true, class: 'form-control' %>
      </div>
    </div>
  </div>    

  <div class="form-group">
    <%= f.submit "Submit", class: 'btn btn-primary' %>
  </div>

<% end %>